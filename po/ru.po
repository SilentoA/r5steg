#: r5steg.py:62
msgid "    [1]: Hide a message."
msgstr "    [1]: Скрыть сообщение."

#: r5steg.py:63
msgid "    [2]: View hidden message inside text."
msgstr "    [2]: Просмотреть скрытое сообщение."

#: r5steg.py:67
msgid "Enter secret message: "
msgstr "Введите секретное сообщение: "

#: r5steg.py:68
msgid "Enter text where the message should be hidden: "
msgstr "Введите текст, где сообщение должно быть скрыто: "

#: r5steg.py:69 r5steg.py:82
msgid "Enter password for encryption: "
msgstr "Введите пароль для шифрования: "

#: r5steg.py:72
msgid "[Hidden Message inside initial string]: "
msgstr "[Скрытое сообщение внутри исходной строки]: "

#: r5steg.py:75
msgid "The result has been copied to clipboard!"
msgstr "Результат скопирован в буфер обмена!"

#: r5steg.py:78
msgid "The result couldn't be copied to clipboard :(, (maybe) missing xclip"
msgstr "Результат не удалось скопировать в буфер обмена :(, (возможно) отсутствует xclip"

#: r5steg.py:81
msgid "Enter text with hidden message: "
msgstr "Введите текст со скрытым сообщением: "

#: r5steg.py:83
msgid ""
"\n"
"[Secret message(s) revealed]:\n"
msgstr ""
"\n"
"[Секретн(ое)(ые) сообщени(е)(я) раскрыт(о)(ы)]:\n"

#: r5steg.py:88
msgid "    Couldn't decrypt message (wrong message/password?):"
msgstr "    Не удалось расшифровать сообщение (неверное сообщение/пароль?):"

#: r5steg.py:92
msgid "Goodbye!"
msgstr "До свидания!"
